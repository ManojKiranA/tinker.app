{
    "name": "spatie/spoon",
    "description": "Our simple Laravel boilerplate.",
    "keywords": [
        "spatie",
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.0",
        "barryvdh/laravel-debugbar": "^2.4",
        "bugsnag/bugsnag-laravel": "^2.6",
        "cboden/ratchet": "^0.4.0",
        "docker-php/docker-php": "dev-2.0-dev",
        "docker-php/docker-php-api": "4.1.25.*",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~3.3",
        "fzaninotto/faker": "~1.4",
        "laravel/framework": "5.5.*",
        "laravel/horizon": "^1.0",
        "laravel/tinker": "~1.0",
        "nunomaduro/collision": "^1.1",
        "pda/pheanstalk": "^3.1",
        "predis/predis": "^1.1",
        "ratchet/pawl": "^0.3.1",
        "react/event-loop": "^0.4.3",
        "react/socket": "^0.8.7",
        "spatie/laravel-backup": "^5.0",
        "spatie/laravel-tail": "^2.0",
        "themsaid/laravel-mail-preview": "^2.0",
        "wrench/wrench": "^2.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "friendsofphp/php-cs-fixer": "^2.4",
        "laravel/envoy": "^1.3",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "deploy": [
            "envoy run deploy"
        ],
        "deploy-code": [
            "envoy run deploy-code"
        ],
        "fix-cs": [
            "vendor/bin/php-cs-fixer fix ."
        ],
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.1"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    }
}
